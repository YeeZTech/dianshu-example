        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;

    /* TODO: Your code here. (Using info fields)

    FieldHelper infoField = get(Fields.Info, "info_field_name");

    RowSet infoStream = findInfoRowSet("info_stream_tag");

    Object[] infoRow = null;

    int infoRowCount = 0;

    // Read all rows from info step before calling getRow() method, which returns first row from any
    // input rowset. As rowMeta for info and input steps varies getRow() can lead to errors.
    while((infoRow = getRowFrom(infoStream)) != null){

      // do something with info data
      infoRowCount++;
    }
    */
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  r = createOutputRow(r, data.outputRowMeta.size());

  /* TODO: Your code here. (See Sample)

  // Get the value from an input field
  String foobar = get(Fields.In, "a_fieldname").getString(r);

  foobar += "bar";

  // Set a value in a new output field
  get(Fields.Out, "output_fieldname").setValue(r, foobar);

  */

 //企业名称
  String OrgName = get(Fields.In, "企业名称").getString(r);
  get(Fields.Out, "org_name_sbc").setValue(r, CharUtil.ToSBC(OrgName));
 //企业类型
  String EntTypeName = get(Fields.In, "企业注册类型").getString(r);
  get(Fields.Out, "ent_type_name_sbc").setValue(r, CharUtil.ToSBC(EntTypeName));
  // 行业名称
  String IndustryName = get(Fields.In, "行业名称").getString(r);
  get(Fields.Out, "industry_name_sbc").setValue(r, CharUtil.ToSBC(IndustryName));
  // 注册地址
  String OrgRegAddr = get(Fields.In, "注册地址").getString(r);
  get(Fields.Out, "org_reg_addr_sbc").setValue(r, CharUtil.ToSBC(OrgRegAddr));
  // 经营地址
  String OrgBizAddr = get(Fields.In, "经营地址").getString(r);
  get(Fields.Out, "org_biz_addr_sbc").setValue(r, CharUtil.ToSBC(OrgBizAddr));
  // Send the row on to the next step.
  putRow(data.outputRowMeta, r);


  return true;
}



public class CharUtil {
    /**
     * 半角转全角
     * @param input String.
     * @return 全角字符串.
     */
    public static String ToSBC(String input) {
			String returnString=null;
			if( input !="" &amp;&amp; input != null){
             char c[] = input.toCharArray();
             for (int i = 0; i &lt; c.length; i++) {
               if (c[i] == ' ') {
                 c[i] = '\u3000';
               } else if (c[i] &lt; '\177') {
                 c[i] = (char) (c[i] + 65248);

               }
             }
 			 returnString = new String(c);
			}
          return returnString;
    }

    /**
     * 全角转半角
     * @param input String.
     * @return 半角字符串
     */
    public static String ToDBC(String input) {
        String returnString=null;
			if( input !="" &amp;&amp; input != null){

             char c[] = input.toCharArray();
             for (int i = 0; i &lt; c.length; i++) {
               if (c[i] == '\u3000') {
                 c[i] = ' ';
               } else if (c[i] > '\uFF00' &amp;&amp; c[i] &lt; '\uFF5F') {
                 c[i] = (char) (c[i] - 65248);

               }
             }
         		returnString = new String(c);
			}

             return returnString;
    }

}

